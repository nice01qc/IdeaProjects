<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="bfpp" class="one.ObscenityRemovingBeanFactoryPostProcessor">
        <property name="obscenties">
            <set>
                <value>bollocks</value>
                <value>winky</value>
                <value>bum</value>
                <value>Microsoft</value>
            </set>
        </property>
    </bean>


    <bean id="simpleBean" class="one.SimplePostProcessor" >
        <property name="connectionString" value="bollocks" />
        <property name="password" value="nice" />
        <property name="username" value="Microsoft" />
    </bean>



    <bean id="child" class="one.ChildBean">
        <property name="child" value="456"/>
        <property name="parentBean" ref="parent"/>
    </bean>

    <bean id="parent" class="one.ParentBean">
        <property name="parent" value="123" />
    </bean>

</beans>