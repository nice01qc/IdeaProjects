<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="one" class="threedimethod.One">
        <constructor-arg type="int" value="2" index="0"/>
        <constructor-arg type="int" value="2" index="1"/>

    </bean>

    <bean id="boy" class="threedimethod.BoyStaticFactory" factory-method="getBoy">
        <constructor-arg value="just your boy"/>
    </bean>


    <bean id="boyInstanceFactory" class="threedimethod.BoyInstanceFactory" >
        <property name="boy" value="just you boy 2" />
    </bean>
    <bean id="boy1" factory-bean="boyInstanceFactory" factory-method="getBoy" />


</beans>