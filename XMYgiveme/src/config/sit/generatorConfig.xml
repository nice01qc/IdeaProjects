<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry
		location="/Users/Song/.m2/repository/mysql/mysql-connector-java/5.1.38/mysql-connector-java-5.1.38.jar" />

	<context id="ad_admin" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://10.209.44.14:10044/ad_admin" userId="ad_admin"
			password="ad_admin">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.wanda.ffanad.crm.domains"
			targetProject="src/main/java" />

		<sqlMapGenerator targetPackage="mybatis"
			targetProject="src/main/resources" />

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.wanda.ffanad.crm.mappers" targetProject="src/main/java" />

		<table schema="ad_admin" tableName="creation_info"
			enableSelectByExample="false" enableDeleteByExample="false"
			enableCountByExample="false" enableUpdateByExample="false" />
		<!-- 
		<table schema="ffanad" tableName="resource_info"
			enableSelectByExample="false" enableDeleteByExample="false"
			enableCountByExample="false" enableUpdateByExample="false" />
		-->
	</context>
</generatorConfiguration>